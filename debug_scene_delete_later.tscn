[sub_resource type="GDScript" id="GDScript_0qie4"]
script/source = "extends Control

@onready
var player : CharacterBody3D = self.get_parent().get_node(\"gs_player\")


func _process(delta: float) -> void:
	get_node(\"is_stuck_label\").text = \"Stuck: \" + str(player.is_stuck)
	get_node(\"is_airborne\").text = \"Airborne: \" + str(player.is_airborne)
	get_node(\"velocity\").text = \"Velocity: \" + str(player.velocity * 100 / 1.905)
	get_node(\"speed\").text = \"Speed: \" + str(int(Vector2(player.velocity.x, player.velocity.z).length() * 100 / 1.905))
	get_node(\"position\").text = \"Pos: \" + str(player.global_position)
	get_node(\"positionHU\").text = \"Pos HU: \" + str(player.global_position * 100 / 1.905)
	
"

